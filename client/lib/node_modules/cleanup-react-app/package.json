{
  "name": "cleanup-react-app",
  "version": "1.1.3",
  "description": "Cleanup your create-react-app. Removes commonly deleted files and boilerplate content from templates generated by create-react-app.",
  "main": "app.js",
  "scripts": {
    "test": "jest --config=jest.config.cjs"
  },
  "bin": {
    "cleanup-react-app": "app.mjs"
  },
  "keywords": [
    "react",
    "create-react-app",
    "delete",
    "react",
    "files"
  ],
  "author": "NiallJP",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "babel-jest": "^26.1.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "filter-obj": "^2.0.1",
    "map-obj": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niallpaterson/cleanup-react-app.git"
  },
  "bugs": {
    "url": "https://github.com/niallpaterson/cleanup-react-app/issues"
  },
  "homepage": "https://github.com/niallpaterson/cleanup-react-app#readme"
}
